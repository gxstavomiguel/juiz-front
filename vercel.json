{
  "framework": "vite",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "env": {
    "VITE_API_BASE_URL": "https://juiz-ai.onrender.com/api/v1",
    "VITE_WS_URL": "wss://juiz-ai.onrender.com"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}